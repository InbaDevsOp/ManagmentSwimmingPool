<div id="addPlan">
  <br>
  <div class="row">
    <div class="col-md-1">
    </div>
    <div class="col-md-10">
      <p class="textTitle">Realizar Pagos</p>
      <div class="line"></div><br>
      <div class="form-group row">
        
        <label for="inputKey" class="col-md-3 control-label textTitleBasic">Digitos del Rut o Apellido Paterno</label>
        <div class="col-md-6" id="user">
          <input type="text" class="form-control" name="userId" id="userId">
        </div>
        <div class="col-md-2">
          <button id="searchUsers" class="btn btn-default botonSearch">Buscar</button>
        </div>
      </div>
      
      <div class="form-group row textTitleBasic">
        <div id="usersInformation" >
        </div>
      </div>
      
      <div id="userDetailInformation" class="textTitleBasic">
      </div>
      
      <div class="form-group row textTitleBasic">
        <div id="plansInformation">
        </div>
      </div>
      
      <div id="planDetailInformation" class="textTitleBasic">
      </div>


      <div class="form-group row textTitleBasic">
        <div id="schedulesInformation">
        </div>
      </div>
      
      <div id="scheduleDetailInformation" class="textTitleBasic">
      </div>
      
    </div>
    <div class="form-group row">
      <div class="col-md-10 botonGuardar">
          <label id="savePlan" onclick="cargar();" class="control-label btn btn-primary">Generar Pago</label>
          <script>
            function cargar(){
              alertDGC("Pago generado con EXITO");
              document.getElementById("userId").value="";
              $("#usersInformation").empty();
              $("#userDetailInformation").empty();
              $("#plansInformation").empty();
              $("#planDetailInformation").empty();
              $("#schedulesInformation").empty();
              $("#scheduleDetailInformation").empty();
          }
          </script>
      </div>
    </div>
  </div>
</div>
<table id="paymentsInformation"class="table table-striped table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>#</th>
      <th>Nombre Plan</th>
      <th>Nombre Horario</th>
      <th>Precio $</th>
      <th>Num.Boleta</th>
      <th>Vigencia</th>
      <th>Fecha de Pago</th>
    </tr>
  </thead>
  <tbody>
    {{#payments}}
    <tr id="{{id}}" class="{{#isProductActive product.startValidDate product.endValidDate}}success{{/isProductActive}}" >
      <td></td>
      <td>{{product.productPK.plan.name}}</td>
      <td>{{product.productPK.schedule.name}}</td>
      <td>{{product.productPK.plan.price}}</td>
      <td>{{numberOfTicket}}</td>
      <td>{{datepay}}</td>
      <td>{{#isProductActive product.startValidDate product.endValidDate}}ACTIVO{{/isProductActive}}</td>
    </tr>
    {{/payments}}
  </tbody>
</table>
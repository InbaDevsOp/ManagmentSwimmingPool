<table id="paymentsInformation"class="table table-striped table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>#</th>
      <th>Nombre Plan</th>
      <th>Nombre Horario</th>
      <th>Fecha Inicio</th>
      <th>Fecha Termino</th>
      <th>Entrar Piscina</th>
      <th>Salir Piscina</th>
    </tr>
  </thead>
  <tbody>
    {{#payments}}
    <tr id="{{id}}" class="{{#isProductActive product.startValidDate product.endValidDate}}success{{/isProductActive}}" >
      <td></td>
      <td>{{product.productPK.plan.name}}</td>
      <td>{{product.productPK.schedule.name}}</td>
      <td>{{product.startValidDate}}</td>
      <td>{{product.endValidDate}}</td>

      <td productId="{{product.id}}" align="center"><a><span id="entrance" class="glyphicon glyphicon-log-in"></span></a></td>  
      <td productId="{{product.id}}" align="center"><a><span id="exit" class="glyphicon glyphicon-log-out"></span></a></td>
   
    </tr>
    {{/payments}}
  </tbody>
</table>